set iconOff to ""
set iconLow to ""
set iconMedium to ""
set iconHigh to ""

tell application "BetterTouchTool"
	set curVolume to get volume settings
	-- 0-33 (1); 34-66 (2); 67-100 (3)

	if output muted of curVolume is true
		return "{\"icon_data\": \"" & iconOff & "\"}"
		-- return "{\"sf_symbol_name\": \"speaker.slash.fill\", \"text\": \"Volume Control\", \"sf_symbol_size\": 24}"
	end if

	set volumeLevel to output volume of curVolume

	if volumeLevel is less than 1 then
		return "{\"icon_data\": \"" & iconOff & "\"}"
		-- return "{\"sf_symbol_name\": \"speaker.slash.fill\", \"text\": \"Volume Control\", \"sf_symbol_size\": 24}"
	else if volumeLevel is less than 34 then
		return "{\"icon_data\": \"" & iconLow & "\"}"
		-- return "{\"sf_symbol_name\": \"speaker.wave.1.fill\", \"text\": \"Volume Control\", \"sf_symbol_size\": 24}"
	else if volumeLevel is less than 67 then
		return "{\"icon_data\": \"" & iconMedium & "\"}"
		-- return "{\"sf_symbol_name\": \"speaker.wave.2.fill\", \"text\": \"Volume Control\", \"sf_symbol_size\": 24}"
	else
		return "{\"icon_data\": \"" & iconHigh & "\"}"
		-- return "{\"sf_symbol_name\": \"speaker.wave.3.fill\", \"text\": \"Volume Control\", \"sf_symbol_size\": 24}"
	end if
end tell
